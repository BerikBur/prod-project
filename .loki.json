{
  "chromeSelector": ".sb-show-main",
  "diffingEngine": "pixelmatch",
  "configurations": {
    "chrome.laptop": {
      "target": "chrome.docker",
      "width": 1366,
      "height": 768
    },
    "chrome.iphone7": {
      "target": "chrome.docker",
      "preset": "iPhone 7"
    }
  },
  "fetchFailIgnore": ".*",
  "chromeDockerWithoutSecurityArgs": true,
  "reactUri": "http://localhost:6006"
}
